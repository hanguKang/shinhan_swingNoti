script.
    // 알림센터 너비 리사이즈
    $(function(){
        var min = 400;
        var max = 700;
        var mainmin = 400;

        $('#split_rgt').mousedown(function (e) {
            e.preventDefault();
            $(document).mousemove(function (e) {
                e.preventDefault();
                var x = $('#sh_notify_lyr').offset().left + $('#sh_notify_lyr').outerWidth() - e.pageX;
                if (x > min && x < max && e.pageX > mainmin) {
                    $('#sh_notify_lyr').css("width", x);
                }
            })
        });

        $(document).mouseup(function (e) {
            $(document).unbind('mousemove');
        });
    });

    $(document).ready(function() {
        UI.init();

        // 별표 아이콘 토글
        $('.service_menu a.star_chk').click(function(){
