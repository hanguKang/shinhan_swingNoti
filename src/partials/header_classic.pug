    function toggleNav(){
        if ($('#layout_wrap').hasClass('fold_nav')) {
            $('#layout_wrap').removeClass('fold_nav');
        } else  {
            $('#layout_wrap').addClass('fold_nav');
        }
    }

    function toggleHd(){
        if ($('.section_helpdesk').hasClass('fold')) {
